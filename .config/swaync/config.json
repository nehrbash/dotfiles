{
    "positionX": "left",
    "positionY": "top",
    "layer": "overlay",
    "timeout": 10,
    "timeout-low": 5,
    "timeout-critical": 0,
    "notification-window-width": 600,
    "keyboard-shortcuts": true,
    "image-visibility": "always",
    "transition-time": 200,
    "hide-on-clear": true,
    "fit-to-screen": false,
    "control-center-width": 600,
    "control-center-height": 600,

    "control-center-margin-bottom": 0,
    "control-center-margin-top": 5,
    "widgets": [
        "notifications",
        "title",
        "dnd"
    ],
    "widget-config": {
        "title": {
            "text": "",
            "clear-all-button": true,
            "button-text": "Clear All"
        },
        "dnd": {
            "text": "Do Not Disturb  "
        },
        "label": {
            "max-lines": 5,
            "text": "Label Text"
        }
    }

}
