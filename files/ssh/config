AddKeysToAgent yes

Host *
    # ControlMaster      auto
    # ControlPath        /tmp/tramp.%C
    # ControlPersist     yes
    StrictHostKeyChecking no
    # IdentityFile /dev/null

# Only load YubiKey PKCS#11 on the host where the library exists.
# Silently skipped inside devcontainers where ~/.guix-home is not mounted.
Match exec "test -f %d/.guix-home/profile/lib/libykcs11.so"
    PKCS11Provider ~/.guix-home/profile/lib/libykcs11.so

